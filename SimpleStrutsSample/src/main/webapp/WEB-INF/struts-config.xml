<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.3//EN" "http://struts.apache.org/dtds/struts-config_1_3.dtd">

<struts-config>
	<form-beans>
		<form-bean name="startForm" type="jp.matsu.sample.struts.form.StartForm" />
	</form-beans>

	<action-mappings>
		<action path="/StartAction" type="jp.matsu.sample.struts.action.StartAction" name="startForm" scope="request">
			<forward name="success" path="/end.jsp" />
		</action>
	</action-mappings>
</struts-config>
